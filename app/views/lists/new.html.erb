<div class='container'>
	<h1> Create your new list here </h1>
	<%= form_for @list, url: {action: "create"}, html: {multipart: true } do |f| %>
	  <%= f.label :title %> <br>
	  <%= f.text_area :title, size: "60x2" %> <br>
	  <%= f.label :description %> <br>
	  <%= f.text_area :description, size: "60x6" %><br><br>
		<p style="padding-left: 40%"><%= f.file_field :avatar %></p> <br>
	  <%= f.submit "Create" %>
	<% end %>
</div>