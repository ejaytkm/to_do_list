<div id='timeNow'>
	<h1>
		<div id='timeSecond'>
			<%= @time %>
		</div>
	</h1>
</div>
<br>

<% @list.each do |x| %>
	Title: <%= x.title %> <br>
	Description: <%= x.description %> <br>
	<%= link_to 'show', user_list_path(id: x.id, user_id: @user.id)%>
	<%= link_to 'edit', edit_user_list_path(id: x.id, user_id: @user.id) %>
	<%= link_to 'delete', user_list_path(user_id: @user.id, list_id: x.id), data: {:confirm => 'Are you sure?'}, :method => :delete %>
	<br>
	<br>
<% end %>


<script>
var time = '<%= @time %>'
function update()
  { 
     $('#timeSecond').replaceWith(time);
     console.log('helo')
  }
$(document).ready(function($){
  window.setInterval('update()', 60000);
  });
</script>